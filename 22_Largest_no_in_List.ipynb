      "source": [
        "def find_largest(numbers):\n",
        "    largest = numbers[0]\n",
        "    for num in numbers:\n",
        "        if num > largest:\n",
        "            largest = num\n",
        "    return largest\n",
        "\n",
        "my_list = [5, 2, 8, 1, 6]\n",
        "result = find_largest(my_list)\n",
        "print(\"The largest number in the list is:\", result)"
      ],
