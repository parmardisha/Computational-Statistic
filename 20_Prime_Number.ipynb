        "def prime_numbers(limit):\n",
        "  primes = []\n",
        "  for num in range(2, limit+1):\n",
        "    for i in range(2, int(num**0.5)+1):\n",
        "      if num % i == 0:\n",
        "        break\n",
        "    else:\n",
        "      primes.append(num)\n",
        "  return primes\n",
        "\n",
        "print(prime_numbers(20))"
      ],
