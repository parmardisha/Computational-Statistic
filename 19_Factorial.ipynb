        "def factorial(n):\n",
        "\n",
        "  if n == 0:\n",
        "    return 1\n",
        "  else:\n",
        "    factorial = 1\n",
        "    for i in range(1, n+1):\n",
        "      factorial *= i\n",
        "    return factorial\n",
        "\n",
        "n = 4\n",
        "result = factorial(n)\n",
        "print(result)"
      ],
