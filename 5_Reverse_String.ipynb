      "source": [
        "def reverse_string(word):\n",
        "  reversed_word = \"\"\n",
        "  for letter in word:\n",
        "    reversed_word = letter + reversed_word\n",
        "  return reversed_word\n",
        "\n",
        "word = input(\"Enter a word: \")\n",
        "reversed_word = reverse_string(word)\n",
        "print(\"Reversed word:\", reversed_word)"
      ],
