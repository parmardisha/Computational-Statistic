      "source": [
        "def factorial(n):\n",
        "    if n == 0:\n",
        "        return 1\n",
        "    else:\n",
        "        return n * factorial(n - 1)\n",
        "\n",
        "num = int(input(\"Enter Number : \"))\n",
        "result = factorial(num)\n",
        "print(\"The factorial of\", num, \"is:\", result)"
      ],
